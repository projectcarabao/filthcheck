    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node -r tsconfig-paths/register dist/index.js",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon --exec ts-node -r tsconfig-paths/register dist/index.js\""
